# ExcelGenius 环境变量配置

# 服务器配置
HOST=127.0.0.1
PORT=8000
DEBUG=True

# DeepSeek API 配置
# 注意：实际使用时请替换为您的真实API密钥
# 此文件不应提交到版本控制系统
DEEPSEEK_API_KEY='your deepseek_api'
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_MAX_TOKENS=2000
DEEPSEEK_TEMPERATURE=0.7

# Excel 配置
EXCEL_MAX_ROWS=1000
EXCEL_MAX_COLS=100

# 上传配置
UPLOAD_MAX_SIZE_MB=10

# 日志配置
LOG_LEVEL=INFO

# 缓存配置
CACHE_ENABLED=True
CACHE_TTL=3600

# 安全配置
API_KEY_REQUIRED=False